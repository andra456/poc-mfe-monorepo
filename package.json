{
  "name": "namespace",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*",
      "services/*"
    ]
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "start": "lerna run start --stream",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "exec:tmpl": "lerna exec --scope ~~name~~ --",
    "exec:ts-tmpl": "lerna exec --scope ~~name-ts~~ --",
    "prerelease": "yarn run build:lib",
    "test:coverage": "lerna run test:coverage",
    "host:dev": "lerna exec --scope host-app -- yarn start",
    "host-assist:dev": "lerna exec --scope host-assist -- yarn dev",
    "remote:dev": "lerna exec --scope module-standard -- yarn dev",
    "remote-next:dev": "lerna exec --scope remote-next -- yarn dev",
    "checkout:dev": "lerna exec --scope checkout -- yarn dev",
    "dashboard:test": "lerna exec --scope dashboard -- yarn test",
    "dashboard:test:coverage": "lerna exec --scope dashboard -- yarn test:coverage",
    "dashboard:deploy": "lerna exec --scope dashboard -- serve -s build -l 3000",
    "mfe:start": "concurrently \"yarn host:dev\" \"yarn remote:dev\" \"yarn costum-next:dev\""
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@loadable/component": "^5.15.3",
    "@loadable/server": "5.16.1",
    "axios": "0.21.1",
    "crypto-js": "^4.1.1",
    "customize-cra": "^0.8.0",
    "emotion": "10.0.27",
    "emotion-server": "10.0.27",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "lodash": "^4",
    "react-app-rewired": "^2.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.9.8",
    "react-redux": "^8.1.1",
    "react-multi-lang": "2.2.0",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "webpack-dev-server": "^4.11.1",
    "dexie": "^3.2.4",
    "dexie-react-hooks": "^1.1.6",
    "react-scripts": "5.0.1",
    "recoil": "0.7.4",
    "typescript": "^5",
    "next": "12.3.4",
    "@swc/helpers": "~0.5.0",
    "@tanstack/query-core": "^4.33.0",
    "antd": "^5.8.6",
    "concurrently": "^7.0.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/gitlab": "12.0.6",
    "@semantic-release/npm": "^11.0.0",
    "@semantic-release/release-notes-generator": "^12.0.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/lodash": "^4",
    "enzyme-to-json": "^3.4.3",
    "eslint": "8.52.0",
    "eslint-config-prettier": "^6.4.0",
    "@typescript-eslint/parser": "6.9.0",
    "@typescript-eslint/eslint-plugin": "6.9.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "@types/loadable__component": "^5.13.5",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.5.6",
    "@types/node": "20.8.2",
    "@types/react": "18.2.24",
    "@types/react-dom": "18.0.10",
    "@babel/preset-env": "^7.23.2",
    "jest": "^24.9.0",
    "lerna": "^4.0.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "./setupTests.ts"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "packages/*/src/**/*.{js,jsx,ts,tsx}"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
