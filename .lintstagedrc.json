{
  "*.{json,md,html}": "prettier --config .prettierrc --ignore-path .prettierignore --write",
  "*.{js,jsx,ts,tsx}": [
    "prettier --config .prettierrc --ignore-path .prettierignore --write",
    "eslint --ignore-path .eslintignore --fix"
  ],

  "*.{png,jpeg,jpg,gif,svg}": ["imagemin-lint-staged"]
}
